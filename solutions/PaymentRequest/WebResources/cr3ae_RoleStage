	if( 
		
	   ( /***********************CIBP********************/
	    idStage=="c6cc371d-997c-4247-8f80-cb87a7fa2a0e" //Review Documentation
		  /***********************REVIEW****TL/RM************/
	    ||idStage=="67e9cc71-69bf-428f-8a05-f49364e71398" //Document Verification
		) && loggedinUserId!=userTL&& loggedinUserId!=userRM){
		
		Xrm.Utility.alertDialog("TL/RM Stage is not allowed for you");
		executionContext.getEventArgs().preventDefault();
		
	}
	
	//RHC **************************
	if( 
		/****************OUTWARD2******************/
	   ( idStage=="4dc8f6b0-1df5-4845-b9fb-086e675fd950" //HOC Cross Verification
	    ||idStage=="1d0d273e-9563-485a-ac9b-4b2e326268e6" //Transaction Reporting Process
	    
	   
		) && loggedinUserId!=userRHC){
		
		Xrm.Utility.alertDialog("RHC Stage is not allowed for you");
		executionContext.getEventArgs().preventDefault();
		
	}
}


function RO (){
	
	//RO ***********************
	if(true){
		return true;
	}
}