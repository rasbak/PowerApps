function RoleStage (executionContext)
{
    debugger;
    var formContext = executionContext.getFormContext();

    formContext.data.process.addOnPreStageChange(Block);
    Xrm.Page.data.process.addOnProcessStatusChange(endBlock);
	var status = Xrm.Page.data.process.getStatus();

	if (status == "finished") {
		Xrm.Page.data.process.removeOnProcessStatusChange(endBlock);
	}
        
}

function endBlock(executionContext){
		debugger;
	var formContext = executionContext.getFormContext();
	
	var loggedinUserId = Xrm.Utility.getGlobalContext().userSettings.userId;
	var userRM; var userRO;var userRC;var userMLRO;var userTL;var userRHC;
	
	if(formContext.getAttribute("ind_rm").getValue())
		userRM = formContext.getAttribute("ind_rm").getValue()[0].id;
	if(formContext.getAttribute("ind_ro").getValue())
		userRO = formContext.getAttribute("ind_ro").getValue()[0].id;
	if(formContext.getAttribute("ind_rc").getValue())
		userRC = formContext.getAttribute("ind_rc").getValue()[0].id; //not added
	if(formContext.getAttribute("ind_mlro").getValue())
		userMLRO = formContext.getAttribute("ind_mlro").getValue()[0].id; //not added
	if(formContext.getAttribute("ind_tl").getValue())
		userTL = formContext.getAttribute("ind_tl").getValue()[0].id; //not added
	if(formContext.getAttribute("ind_rhc").getValue())
		userRHC = formContext.getAttribute("ind_rhc").getValue()[0].id; //not added
	
	 var idStage = Xrm.Page.data.process.getActiveStage().getId();
	// var currentRole = formContext.getAttribute("ind_currentrole").getValue();
	var status = Xrm.Page.data.process.getStatus();
	 
	
	//RO ***********************
	if(
	/*******************TRANSACTION***************/
	(idStage=="51e9c646-964e-47f3-ae0b-97568db7188f" //Inward/Outward
	/***********TR*******************/
	||idStage=="48c5db30-fd06-436b-958a-fac605af73aa" //EDD
	||idStage=="f0defd43-1099-4e08-995f-25dc1df0da53" //Transaction reporting
	||idStage=="3519469f-e5ba-40c2-9147-43e5f6177c41"||idStage=="6bef052b-48cb-4ab8-97dc-efb1e7db2246" //Proceed With Transaction Monitoring
	||idStage=="3f1a062a-8948-4d58-97c6-3f45db6682e1" ||idStage=="19f20bd1-a5c9-4722-ac67-2749eec21cfe" ||idStage=="205fa648-f9a0-4442-aca3-7ef01a95d992" //STR
	/*********OUTWARD1*****************/
	||idStage=="26c7d2a9-eae3-4582-aac1-8b990967cab8" // Proceed With  CDD Screening
	||idStage=="7869b070-b66d-4720-8cc5-95829aa2c74a" // Proceed With Asses Deviation
	/***********ARA********************/
	||idStage=="af353c5b-6772-482a-93a1-e149d47e2a2f" //Transaction Reporting Process
	||idStage=="e8e83579-0e40-4986-9452-ec0d56ea2b5b" //Proceed With Transaction Monitoring
	||idStage=="7a0b2b1f-e8aa-4088-9f8b-eff170f9c040" //STR Process	
	/************JURISDICTION***************/
	||idStage=="8e7b33d3-778b-4f60-bf86-520d8f6fcc0f" // Transaction Reporting Process
	||idStage=="2a68bc35-ed64-4ee5-8797-849efeea6828" //Proceed With Transaction Monitoring
	/************cddi*********************/
	||idStage=="1eac60c5-07e7-48fe-8d91-fb9a724894ed"||idStage=="89b72376-d6ed-4a14-b374-976e871999fe" // CDD Screening
	||idStage=="67663308-6359-4f99-85d8-acaeae34cece" ||idStage=="8f66f957-c51f-4d4d-aa25-bbab1fff497b" // CDD Screening
	||idStage=="15cc60d0-7427-47bd-8a3d-bd714557b2d5" //Transaction Reporting Process
	||idStage=="561ab389-3df6-433a-800d-9130516e32f9" ||idStage=="12d07c52-c09e-4800-9a57-b47aa3324436" //STR Process
	/*********ONGOING***************/
	||idStage=="47bbfef6-702d-4e3f-8dcf-018bc30e8db9" // EDD Process
	||idStage=="f21f684c-e53a-4bec-aa10-44584d7b3995" // STR Process
	||idStage=="264a8247-fd05-4f15-9e1a-d8d026e3ab97" // PEP Screening
	||idStage=="e4a3ee7f-42c7-4a7b-b709-80a5676ec3b3" ||idStage=="05834b5a-de0a-4cc0-8080-e6fd9ea00bf5" ||idStage=="7627ad76-3949-48c1-995d-9ea7ab16489c" //Continue With Transaction Monitoring Process
	/*****CDDS**************/
	||idStage=="8da809d0-a6da-4da1-a454-51223cd1121b" // Initiate STR Process
	||idStage=="4db0de07-13c6-4f40-ab26-13a2d7376e55" // PEP Screening Process
	||idStage=="3e0ec563-b0b6-421e-a73e-5a6d4839d40e" //EDD Process
	||idStage=="d2148a9c-1c4c-44e5-a1a6-966f987f5592"||idStage=="4f954a17-55fc-4170-87b6-be15f6fa7350" //Continue with Transaction Review
	
	
	/************PEP***********/
	||idStage=="62d3461f-cafe-43e2-9ff7-0bc8eb1b85cc" // Transaction Reporting Process
	||idStage=="b95a4d48-2a2f-4074-8937-61b567457b55" // EDD Process
	) && loggedinUserId!=userRO && status=="finished"){
		
		
		Xrm.Utility.alertDialog("RO Stage is not allowed for you");
		formContext.data.process.setStatus('active');
		
	}
	
	if ( idStage=="ffe04f69-febd-4339-aa62-2b402f04bed1" /*Ongoing Screening*/ && loggedinUserId!=userRC && status=="finished"){
		Xrm.Utility.alertDialog("RC Stage is not allowed for you");
		formContext.data.process.setStatus('active');
	}
}




