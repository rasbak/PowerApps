function RegenerateDoc(executionContext){
debugger;
	var formContext = executionContext.getFormContext();
	var paymentID = formContext.data.entity.getId();
			const response = new XMLHttpRequest();
			const json = JSON.stringify({
				id: paymentID		
	});

		response.open("POST", 'https://prod-18.westus.logic.azure.com:443/workflows/350623d4e05648a4b27e1744c57cae77/triggers/manual/paths/invoke?api-version=2016-06-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=pFWSzTLGqaFKQqY7yhPbN8GXpXetdD1Fdo-1pSpNy4E')
		response.setRequestHeader('Content-Type', 'application/json');

		response.send(json);
		
		
		Xrm.Utility.alertDialog("You will not be able to proceed until file is ready, please wait few seconds!");
		formContext.data.process.addOnPreStageChange(myFunction);
		
		setTimeout(() => { 
			Xrm.Utility.alertDialog("You can proceed. Document is ready!");
			formContext.data.process.removeOnPreStageChange(myFunction);
		}, "25000");


}

function myFunction(executionContext){
	debugger;
	executionContext.getEventArgs().preventDefault();
}